    "lint-fix": "eslint --fix",
    "lint-check": "eslint .",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "create-hook": "husky add .husky/pre-commit \"yarn run pre-commit\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
